package Model;

/**
 *
 * @author chris
 */
public class Tax extends Space {
    
    private int taxCost;
    
    public Tax(String name, int taxAmount) {
        super(name, -1, -1);
        this.taxCost = taxAmount;
        this.setOwner(Bank.getInstance());
    }
    
    public int getRent(int dice) {
        return taxCost;
    }
    
}
